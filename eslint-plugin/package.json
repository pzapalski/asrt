{
  "name": "@asrt/eslint-plugin",
  "version": "1.0.6",
  "description": "Eslint plugin for asrt package.",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p src/tsconfig.json",
    "build:watch": "tsc -p src/tsconfig.json -w",
    "test": "npm run build && jasmine --config=jasmine.json",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pzapalski/asrt.git"
  },
  "author": "Piotr Zapalski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pzapalski/asrt/issues"
  },
  "homepage": "https://github.com/pzapalski/asrt#readme",
  "dependencies": {
    "randexp": "^0.5.3"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "@typescript-eslint/types": "^4.2.0",
    "eslint": "^7.1.0",
    "jasmine": "^3.6.1",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "**/*.js",
    "**/*.js.map",
    "**/*.d.ts",
    "!spec/**/*.*"
  ]
}
